.display
.d
  &--block
  &--b
    display: block

  &--inline-block
  &--ib
    display: inline-block

  &--flex
  &--fl
    display: flex

    flex-helpers-container($grid.suffix)

  &--inline-flex
  &--ifl
    display: flex

    flex-helpers-items()

  &--table
  &--tb
    display: table

  &--table-cell
  &--tb-cl
    display: table-cell

  &--table-row
  &--tb-rw
    display: table-row

